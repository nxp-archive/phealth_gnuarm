	echo "dir ${srcdir}" >> .gdbinit
	if [ x$gdb_needs_out_file_path = xyes ]
	then
		echo "dir ${srcdir}/config/"`dirname ${out_file}` >> .gdbinit
	fi
	if [ "x$subdirs" != x ]; then
		for s in $subdirs
		do
			echo "dir ${srcdir}/$s" >> .gdbinit
		done
	fi
	echo "source ${srcdir}/.gdbinit" >> .gdbinit
fi

